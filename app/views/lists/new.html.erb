<div class="container">
<h1>Create a new list</h1>
  <div class="row">
    <div class="col-xs-12 col-sm-12">
    <%= simple_form_for [@list] do |f| %>
      <%= f.input :name %>
      <%= f.submit class: "btn btn-primary" %>
    <% end %>
    </div>
  </div>
  <p> <%= link_to "Back to list", lists_path %></p>
</div>
